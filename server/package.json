{
  "name": "scheduling-assistant-server",
  "version": "1.0.0",
  "description": "Backend server for Andrea's scheduling assistant",
  "main": "dist/server.js",
  "scripts": {
    "start": "NODE_NO_WARNINGS=1 npm run build && node dist/server.js",
    "start:prod": "NODE_NO_WARNINGS=1 node dist/server.js",
    "dev": "NODE_NO_WARNINGS=1 nodemon src/server.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:prod": "node scripts/migrate-and-exit.js",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "import": "tsx scripts/import-data.ts",
    "import:historical": "tsx scripts/import-historical-data.ts",
    "clear-db": "tsx scripts/clear-work-activities.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@google-cloud/local-auth": "^3.0.1",
    "@notionhq/client": "^3.1.3",
    "@types/express-session": "^1.18.2",
    "@types/multer": "^1.4.13",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "axios": "^1.5.0",
    "commander": "^12.0.0",
    "connect-session-sequelize": "^8.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.44.2",
    "express": "^4.18.0",
    "express-session": "^1.18.1",
    "google-auth-library": "^9.0.0",
    "googleapis": "^126.0.0",
    "helmet": "^7.0.0",
    "intuit-oauth": "^4.2.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "node-quickbooks": "^2.0.46",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.0",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "nodemon": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0"
  }
}
